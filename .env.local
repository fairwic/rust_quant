# 🚀 Rust Quant 本地开发环境配置

# ========== 应用环境 ==========
APP_ENV=local

# ========== 数据库配置 ==========
# ⚠️ 使用 DATABASE_URL（新架构要求）
DATABASE_URL=mysql://root:example@127.0.0.1:33306/test
DATABASE_MAX_CONNECTIONS=10

# ========== Redis 配置 ==========
REDIS_URL=redis://127.0.0.1:6379
REDIS_POOL_SIZE=20

# ========== OKX API 配置 ==========
REST_URL=https://aws.okx.com
WS_PUBLIC_URL=wss://wspap.okx.com:8443/ws/v5/public?brokerId=9999
WS_PRIVATE_URL=wss://wspap.okx.com:8443/ws/v5/private?brokerId=9999
WS_BUSINESS_URL=wss://wspap.okx.com:8443/ws/v5/business?brokerId=9999

# OKX API 密钥（如果有的话）
# OKX_API_KEY=
# OKX_SECRET_KEY=
# OKX_PASSPHRASE=

# ========== 功能开关 ==========
IS_RUN_SYNC_DATA_JOB=false
IS_BACK_TEST=false
IS_BACK_TEST_NWE=false
IS_OPEN_SOCKET=false
IS_RUN_REAL_STRATEGY=false

# ========== 策略配置 ==========
RUN_STRATEGY_PERIOD=5m
ENABLE_RANDOM_TEST=false

# ========== 日志配置 ==========
RUST_LOG=info
LOG_TO_FILE=false
